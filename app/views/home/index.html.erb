<div class="jumbotron">
  <h1>Blocmarks</h1>
  <h4>Social Bookmarking</h4>
  <% unless current_user %>
    <%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook), class: 'btn btn-primary' %><br/>
    <%= link_to "Sign in", new_user_session_path, class: 'btn btn-default' %>
    <%= link_to "Sign up", new_user_registration_path, class: 'btn btn-danger' %>
  <% end %>
</div>
